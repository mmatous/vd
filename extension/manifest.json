{

"description": "Verify downloaded files against provided digests.",
"homepage_url": "https://github.com/mmatous/vd",
"manifest_version": 2,
"name": "vd",
"version": "0.4.0",

"applications": {
	"gecko": {
		"id": "vd@vd.io",
		"strict_min_version": "67.0"
	}
},

"background": {
	"page": "vd-background.html"
},

"options_ui": {
	"browser_style": true,
	"page": "options/vd-options.html"
},

"permissions": [
	"<all_urls>",
	"downloads",
	"menus",
	"nativeMessaging",
	"notifications",
	"storage"
]

}
